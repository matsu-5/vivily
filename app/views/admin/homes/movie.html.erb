<%= link_to 'New Video', new_admin_video_path,class: "btn btn-lg, btn-primary" %>

<div class="container">
	<div class="row">
        <div class="col-md-9 col-md-offset-0">
		  <h2 class="new-video">Video</h2>
          <% @video.each do |video| %>
            <image class="img-fluid">
            	<%= link_to admin_video_path(video) do %>
			    <%= attachment_image_tag video, :image, size: "200x200" %>
			    <% end %>
			</image>
          <% end %>
        </div>
    </div>
</div>

<%= link_to 'New movie', new_admin_movie_path,class: "btn btn-lg, btn-primary new-button" %>

<div class="container">
    <div class="row">
        <div class="col-md-9 col-md-offset-0">
        <h2 class="new-movie">Movie</h2>
        <% @movie.each do |movie| %>
            <image class="img-fluid">
            	<%= link_to admin_movie_path(movie) do %>
			    <%= attachment_image_tag movie, :image, size: "200x200" %>
			    <% end %>
			</image>
        <% end %>
        </div>
    </div>
</div>