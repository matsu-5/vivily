<div class="top_image">
  <section class="container clearfix">
  <div class="row">
    <div class="col-md-8 col-md-offset-2 new-container">
      <h1 class="new-video text-center">Update Video</h1>
      <hr>
      <div class="col-md-9 col-md-offset-4">
        <div class = "row">
        <%= form_for @video,:url => {:action => :update} do |f|%>
          <b>Video Title</b>
          <%= f.text_field :title, class:"form-control", placeholder: "Video Title" %>
          <b>Image</b>
          <%= f.attachment_field :image %>
          <b>Video</b>
          <%= f.file_field :video %>
          <div class="new-actions">
          <%= f.submit "update", class:"btn btn-dark btn-lg btn-block" %>
        </div>
        <% end %>
          <div class="col-md-9 col-md-offset-2">
                    <div class ="back">
                      <%= link_to 'Back', admin_movie_path, class: "btn-default" %>
                    </div>
                </div>
        </div>
      </div>
    </div>
  </div>
  </section>
</div>