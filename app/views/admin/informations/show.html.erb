     <div class="container">
        <div class="row">
          <h1 class="new-information">information</h1>
              <div class="example">
                <p><%= @information.created_at.strftime('%Y/%m/%d') %></p><br>
                <div class ="information title">
                  <%= @information.title %>
                </div>
                <hr>
                <p><%= @information.details.html_safe %></p>
              </div>
              <div class="action">
                <%= link_to "Edit", edit_admin_information_path(@information), class:"btn-sm btn-dark"%>
                <%= link_to "Destroy", admin_information_path(@information),class:"btn-sm btn-danger", method: :delete, "data-confirm" => "Are you sure?" %>
              </div>
              <hr>
          </div>
      </div>