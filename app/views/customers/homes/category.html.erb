<div class="top_image">
  <div class="content">
    <div class="container">
	  <div class="row">
		<div class="col-md-9 col-md-offset-0">
	       <h2 class="new-disc">discography</h2>
	       <nav>
               <ul class="float-left">
      	         <li>
	               <%= @category.name %>
                </li>
               </ul>
             </nav>
		   <% @discs.each do |disc| %>
		     <image class="example_image">
		  	     <%= link_to admin_disc_path(disc) do %>
			     <%= attachment_image_tag disc, :image, size: "200x200" %>
			     <% end %>
		     </image>
		   <% end %>
	    </div>
	  </div>
    </div>
  </div>
</div>