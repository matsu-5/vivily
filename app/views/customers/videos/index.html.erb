<div class="top_image">
  <div class="content">
    <div class="container">
	  <div class="row">
        <div class="col-md-9 col-md-offset-0">
		  <h2 class="new-video">Video</h2>
          <% @video.each do |video| %>
            <image class="img-fluid">
            	<%= link_to customers_video_path(video) do %>
			    <%= attachment_image_tag video, :image, size: "200x200" %>
			    <% end %>
			</image>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
