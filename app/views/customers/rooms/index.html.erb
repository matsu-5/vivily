<div class ="second_image">
  <div class="content">
  <h1 class="text-center">チャットルーム一覧</h1>
    <div class="container">
      <div class ="text-color">
	    <div class="row">
		    <div class="col-8 offset-2">
			    <% @anotherEntries.each do |entry| %>
				    <table class="table table-striped">
					    <tr>
						    <td>
  							  <%= link_to customers_room_path(entry.room_id) do %>
    						<!--%= attachment_image_tag entry.user, :image, :fill, 50, 50, format:'jpeg', fallback: "no_image.jpeg", size:'50×50' %-->
    							<%= entry.customer.first_name %><%= entry.customer.last_name %>
  							  <% end %>
  						  </td>
  						  <td>
  							  <% direct_message = DirectMessage.find_by(id: entry.room.direct_message_ids.last) %>
  							<!--%= truncate(direct_message,length: 10) %-->
  						  </td>
  							  <%= entry.updated_at.strftime('%Y/%m/%d') %>
  						  </td>
  						  <td>
  							  <%= entry.updated_at.strftime('%H:%M') %>
  						  </td>
  				    </tr>
  				  </table>
			    <% end %>
        </div>
		  </div>
	  </div>
  </div>
</div>
</div>